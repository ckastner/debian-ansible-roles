{% if debci_amqp_server | length > 0 -%}
debci_amqp_server="{{ debci_amqp_server }}"
{% endif %}
{% if debci_amqp_ssl %}
debci_amqp_ssl=true
debci_amqp_cacert=/etc/debci/ca.crt
debci_amqp_cert=/etc/debci/client.crt
debci_amqp_key=/etc/debci/client.key
debci_mirror="{{ debci_mirror }}"
{% endif %}
{% if debci_arch | length > 0 %}
debci_arch="{{ debci_arch }}"
{% endif %}
{% if debci_suite | length > 0 %}
debci_suite="{{ debci_suite }}"
{% endif %}
{% if debci_backend | length > 0 %}
debci_backend="{{ debci_backend }}"
{% endif %}
